pipeline {
  agent any
  stages {
    stage('cleanup') {
      steps {
        sh 'docker system prune -a --volumes --force'
      }
    }
    stage('build image') {
      steps {
        sh 'docker build -t ghcr.io/bkumar08/btest:ghcrtest .'
      }
    }
    stage('list docker image') {
     steps {
       sh 'docker images'
     }  
  }
}
